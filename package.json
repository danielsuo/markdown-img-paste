{
  "name": "markdown-img-paste",
  "main": "./lib/markdown-img-paste",
  "version": "0.3.2",
  "description": "Just use 'ctrl+shfit+v' paste image into markdown",
  "keywords": [
    "markdown",
    "md",
    "image",
    "paste"
  ],
  "activationCommands": {
    "atom-workspace": "markdown-img-paste:paste"
  },
  "repository": "https://github.com/cocoakekeyu/markdown-img-paste",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "qiniu": "^6.1.11",
    "request": "^2.55.0"
  },
  "configSchema": {
    "only_markdown": {
      "type": "boolean",
      "default": true,
      "title": "Only in markdown grammar to enable the plugin",
      "description": ""
    },
    "upload_to_mssm": {
      "type": "boolean",
      "default": false,
      "title": "Use sm.ms for image link",
      "description": ""
    },
    "upload_to_qiniu": {
      "type": "boolean",
      "default": false,
      "title": "Use qiniu for image link",
      "description": ""
    },
    "zAccessKey": {
      "type": "string",
      "default": "",
      "title": "AccessKey",
      "description": ""
    },
    "zSecretKey": {
      "type": "string",
      "default": "",
      "title": "SecretKey",
      "description": ""
    },
    "zbucket": {
      "type": "string",
      "default": "",
      "title": "Bucket",
      "description": ""
    },
    "zdomain": {
      "type": "string",
      "default": "http://",
      "title": "Domain",
      "description": ""
    }
  }
}
